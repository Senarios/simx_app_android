<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="45"
           android:pivotX="0"
            android:pivotY="0"
            >
            <shape android:shape="rectangle" >
                <stroke
                    android:width="10dp"
                    android:color="@color/primary" />

                <solid android:color="@color/primary" />
            </shape>
        </rotate>
    </item>
</selector>